<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kimym.onsopt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BlackTheme">
        <activity android:name=".recycler.RecyclerActivity" />
        <activity android:name=".recycler.detail.DetailActivity" />
        <activity android:name=".recycler.grid.GridActivity" />
        <activity android:name=".recycler.linear.LinearActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".signup.SignUpActivity" />
        <activity android:name=".signin.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>